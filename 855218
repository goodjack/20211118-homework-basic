#include <iostream>
using namespace std;
int main() {
	int h;
  cin >> h;
  for (int i=0;i<h;i++){
    for (int j=0;j<-i+h;j++){
      cout << "0";
    }
    for (int j=0;j<i;j++){
      cout << "*";
    }
    cout << "  ";
    for (int k=0;k<i;k++){
      cout << "*";
    }
    cout << "\n";
  }
}
